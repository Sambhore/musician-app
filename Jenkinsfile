pipeline {
    agent any

    stages {
        stage('file path') {
            steps {
               echo 'mvn test'
            }
        }
        
        stage('-----test----') {
            steps {
               echo 'mvn test'
            }
        }
        
        stage('-----package----') {
            steps {
               echo 'mvn package'
            }
        }
    }
}
